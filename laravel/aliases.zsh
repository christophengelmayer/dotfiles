alias laravel='composer create-project --prefer-dist laravel/laravel'

alias a='php artisan'

alias am:c="php artisan make:controller"
alias am:m="php artisan make:model"
alias am:migration="php artisan make:migration"
alias am:auth="php artisan make:auth"
alias am:seeder="php artisan make:seeder"

alias migrate="php artisan migrate"
alias migrate:r="php artisan migrate:refresh"
alias migrate:rs="php artisan migrate:refresh --seed"

